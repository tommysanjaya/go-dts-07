package main

import (
	"fmt"
)

func main() {
	uniChar := [] rune {'С','А','Ш','А','Р','В','О'}

	for i := 0; i < 5; i++{
		fmt.Printf("Nilai i = %v\n", i)
	}

	for j := 0; j < 11; j++ {
		fmt.Printf("Nilai j = %v\n", j)
		if j == 4{
			for k := 0; k < len(uniChar); k++{
					fmt.Printf("character %U '%c' starts at byte position %v\n", uniChar[k], uniChar[k], k*2)
			}
			j++
		}
	}
  
}
