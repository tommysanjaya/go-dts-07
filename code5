package main

import (
	"fmt"
	"math/rand"
	"time"
)

func printData(data []interface{}, id int) {
	for i := 0; i < 1; i++ {
		fmt.Println(data, id)
		time.Sleep(time.Duration(rand.Intn(1000)) * time.Millisecond)
	}
}

func main() {
	data1 := []interface{}{"bisa1", "bisa2", "bisa3"}
	data2 := []interface{}{"coba1", "coba2", "coba3"}

	for i := 1; i <= 4; i++ {
		go printData(data1, i)
		go printData(data2, i)
	}

	time.Sleep(3 * time.Second)
}
