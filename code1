package main

import "fmt"

func main() {
	i := 21
	var j bool = true
	var k float64 = 123.456;

	fmt.Printf("%v\n", i)
	fmt.Printf("%T\n", i)
	fmt.Println("%")
	fmt.Printf("%v\n", j)
	fmt.Printf("%c\n", '\u042F')
	fmt.Printf("%d\n", 21)
	fmt.Printf("%o\n", 21)
	fmt.Printf("%x\n", 15)
	fmt.Printf("%c\n", 0x46)
	fmt.Printf("%U\n", 'Ð¯')
	fmt.Printf("%f\n", k)
	fmt.Printf("%e\n", k)
}
